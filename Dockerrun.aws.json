{
  "AWSEBDockerrunVersion":2,
  "containerDefinitions":[
    {
      "name":"catulam-db",
      "image":"timt1/catulam-db",
      "portMappings":[
        {
          "hostPort":7474,
          "containerPort":7474
        },
        {
          "hostPort":7473,
          "containerPort":7473
        },
        {
          "hostPort":7687,
          "containerPort":7687
        }
      ],
      "environment":[
        {
          "name":"NEO4J_AUTH",
          "value":"neo4j/letmein"
        },
        {
          "name":"NEO4J_dbms_security_procedures_unrestricted",
          "value":"apoc.*"
        },
        {
          "name":"NEO4J_apoc_import_file_enabled",
          "value":true
        },
        {
          "name":"NEO4J_apoc_export_file_enabled",
          "value":true
        },
        {
          "name":"NEO4J_dbms_shell_enabled",
          "value":true
        },
        {
          "name":"NEO4J_apoc_ttl_enabled",
          "value":true
        },
        {
          "name":"NEO4J_apoc_ttl_schedule",
          "value":10
        },
        {
          "name":"NEO4JLABS_PLUGINS",
          "value":"[\"apoc\"]"
        }
      ],
      "essential":true,
      "memory":512
    },
    {
      "name":"catulam-api",
      "image":"timt1/catulam-api",
      "portMappings":[
        {
          "hostPort":4000,
          "containerPort":4000
        }
      ],
      "environment":[
        {
          "name":"NEO4J_URI",
          "value":"bolt://neo4j:7687"
        },
        {
          "name":"NEO4J_USER",
          "value":"neo4j"
        },
        {
          "name":"NEO4J_PASSWORD",
          "value":"letmein"
        },
        {
          "name":"GRAPHQL_LISTEN_PORT",
          "value":4000
        },
        {
          "name":"JWT_SECRET",
          "value":"catulam2A34iaeDwefdAaawe12s0wfha048"
        },
        {
          "name":"BCRYPTHASHCOST",
          "value":10
        },
        { "name": "CORS_ORIGIN",
          "value": "http://catulam1.eu-west-2.elasticbeanstalk.com"
        },
        { "name": "GRAPHQL_URI",
          "value": "http://api:4000/graphql"
        },
        { "name": "NODE_ENV",
          "value": "production"
        }
      ],
      "essential":false,
      "memory":128
    },
    {
      "name":"catulam-frontend",
      "image":"timt1/catulam-frontend",
      "portMappings":[
        {
          "hostPort":80,
          "containerPort":80
        }
      ],
      "memory":128,
      "essential":false
    }
  ]
}
