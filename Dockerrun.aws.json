{
  "AWSEBDockerrunVersion":2,
  "containerDefinitions":[
    {
      "name":"catulam-api",
      "image":"timt1/catulam-api",
      "portMappings":[
        {
          "hostPort":4000,
          "containerPort":4000
        }
      ],
      "environment":[
        {
          "name":"NEO4J_URI",
          "value":"bolt://neo4j:7687"
        },
        {
          "name":"NEO4J_USER",
          "value":"neo4j"
        },
        {
          "name":"NEO4J_PASSWORD",
          "value":"letmein"
        },
        {
          "name":"GRAPHQL_LISTEN_PORT",
          "value":4000
        },
        {
          "name":"JWT_SECRET",
          "value":"catulam2A34iaeDwefdAaawe12s0wfha048"
        },
        {
          "name":"BCRYPTHASHCOST",
          "value":10
        },
        {
          "name":"NODE_ENV",
          "value":"production"
        },
        { "name": "CORS_ORIGIN",
          "value": "http://catulam-demo.eu-west-2.elasticbeanstalk.com"
        },
        { "name": "GRAPHQL_URI",
          "value": "http://catulam-demo.eu-west-2.elasticbeanstalk.com:4000/graphql"
        }
      ],
      "essential":false,
      "memory":128
    },
    {
      "name":"catulam-frontend",
      "image":"timt1/catulam-frontend",
      "portMappings":[
        {
          "hostPort":80,
          "containerPort":80
        }
      ],
      "memory":128,
      "essential":false
    }
  ]
}
