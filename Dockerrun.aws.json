{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "catulam-db",
      "image": "timt1/catulam-db",
      "portMappings": [
        {
          "hostPort": 7474,
          "containerPort": 7474
        },
        {
          "hostPort": 7687,
          "containerPort": 7687
        }
      ],
      "environment": [
        {
          "name": "NEO4J_AUTH",
          "value": "neo4j/letmein"
        },
        {
          "name": "NEO4J_dbms_security_procedures_unrestricted",
          "value": "apoc.*"
        }
      ],
      "essential": true,
      "memory": 128
    },
    {
      "name": "catulam-api",
      "image": "timt1/catulam-api",
      "portMappings": [
        {
          "hostPort": 7000,
          "containerPort": 7000
        }
      ],
      "environment":[
        {
        "name": "NEO4J_URI",
        "value": "bolt://neo4j:7687"
        }
      ],
      "essential": false,
      "memory": 128
    },
    {
      "name": "catulam-frontend",
      "image": "timt1/catulam-frontend",
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "essential": false,
      "memory": 128
    }
  ]
}
